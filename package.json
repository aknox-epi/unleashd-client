{
  "name": "unleashd-client",
  "main": "expo-router/entry",
  "version": "0.1.1",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export --platform web --output-dir dist",
    "build:preview": "expo export --platform web --output-dir dist",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage --maxWorkers=2 --passWithNoTests",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prepare": "husky",
    "release": "commit-and-tag-version",
    "release:minor": "commit-and-tag-version --release-as minor",
    "release:major": "commit-and-tag-version --release-as major",
    "release:patch": "commit-and-tag-version --release-as patch",
    "release:first": "commit-and-tag-version --first-release",
    "release:dry": "commit-and-tag-version --dry-run"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/html-elements": "^0.12.5",
    "@expo/vector-icons": "^15.0.2",
    "@gluestack-ui/core": "^3.0.8",
    "@gluestack-ui/utils": "^3.0.11",
    "@gorhom/bottom-sheet": "^5.0.0-alpha.11",
    "@legendapp/motion": "^2.4.0",
    "@react-aria/utils": "^3.29.0",
    "@react-navigation/native": "^7.1.6",
    "babel-plugin-module-resolver": "^5.0.2",
    "dom-helpers": "^5.2.1",
    "expo": "^54.0.7",
    "expo-constants": "^18.0.10",
    "expo-font": "~14.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.4",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "lucide-react-native": "^0.510.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-aria": "^3.41.1",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.5.1",
    "react-stately": "^3.39.0",
    "tailwind-variants": "^0.1.20",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "commit-and-tag-version": "^12.6.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.11",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
